{
   "background": {
      "scripts": [ "js/jquery-1.12.4.js", "js/common.js", "js/background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "19": "images/icon-19.png",
         "38": "images/icon-19@2x.png"
      },
      "default_popup": "popup.html",
      "default_title": "Zoom Scheduler"
   },
   "content_scripts": [ {
      "css": [ "css/content.css" ],
      "js": [ "js/jquery-1.12.4.js", "js/jquery.simplemodal.js", "js/combobox.js", "js/goog-script.js", "js/content.js" ],
      "matches": [ "https://www.google.com/calendar/*", "https://calendar.google.com/calendar/*", "https://*.zoom.us/google/oauth/*", "https://*.zoom.us/saml/extension/*" ]
